digraph {
    label="the current governor sets up a library"
    node [ colorscheme=pastel19 style=filled ]

    /* Entities */
    governor [ color=2 ]
    treasury [ color=4 ]
    librarian [ color=6 ]

    /* Relationships */
    governor -> treasury [ label="1\l op::proxy_set_library\l 0.10 TON\l" ]
    treasury -> librarian [ label="2\l op::set_library\l 0.09 TON\l"]

    /* Ranks */
    { rank=min governor }
}
